                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        OrganisationID, OrganisationName, OrganisationTypeID, 
                         CASE WHEN OrganisationTypeID = 1 THEN 'Lead organisation' ELSE 'Supported organisation' END AS OrganisationType, SponsorOrganisationID, 
                         COALESCE
                             ((SELECT        OrganisationName
                                 FROM            isp_Organisations AS OSQ
                                 WHERE        (OrganisationID = O.SponsorOrganisationID)), '') AS SponsorOrganisation, OrganisationAddress,
                             (SELECT        LoweredEmail
                               FROM            aspnet_Membership
                               WHERE        (UserId = O.ISPFirstRegisteredBy)) AS RegisteredBy, ISPFirstRegisteredDate, InactivatedDate, ISPFirstRegisteredBy, ICORegistrationNumber, 
                         RegEvidenceFileID,
                             (SELECT        f.FileName + ' (' + CAST(f.FileSize AS varchar) + ' KB)' AS FileName
                               FROM            isp_Files AS f INNER JOIN
                                                         isp_Organisations AS org ON f.FileID = org.RegEvidenceFileID
                               WHERE        (org.OrganisationID = O.OrganisationID)) AS RegEvidenceFile, Longitude, Latitude, OrgContactPhone, OrgContactEmail, OrgContactName, 
                         RequestClosureDate, RequestClosureReason, County, AdminGroupID, Aliases, Identifiers, COALESCE
                             ((SELECT        GroupName
                                 FROM            isp_AdminGroups
                                 WHERE        (AdminGroupID = O.AdminGroupID)), 'None') AS AdminGroup, OrganisationCategoryID, COALESCE
                             ((SELECT        OrganisationCategory
                                 FROM            isp_OrganisationCategories
                                 WHERE        (OrganisationCategoryID = O.OrganisationCategoryID)), 'None') AS OrganisationCategory, OrganisationCategoryOther, PrivacyNoticeURL,
                             (SELECT        dbo.CheckOrganisationPrivacyNoticeExists(@OrganisationID) AS Expr1) AS HasPN, 
                         CASE WHEN O.DPOExempt = 1 THEN 'DPO not required' ELSE COALESCE
                             ((SELECT        TOP (1) OrganisationUserName
                                 FROM            isp_OrganisationUsers
                                 WHERE        (OrganisationID = @OrganisationID) AND (RoleID = 10) AND (Active = 1) AND (ResignDate IS NULL)
                                 ORDER BY Confirmed DESC, Validated DESC, AddedDate DESC), 'No DPO specified') END AS DPO
FROM            isp_Organisations AS O
WHERE        (OrganisationID = @OrganisationID)</CommandText>
