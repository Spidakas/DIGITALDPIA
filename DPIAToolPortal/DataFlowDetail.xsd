              <DbSource ConnectionRef="csISPData (MySettings)" DbObjectName="ispdata.dbo.isp_DataFlowDetail" DbObjectType="Table" GenerateMethods="Get" GenerateShortCommands="true" GeneratorGetMethodName="GetDataForOrganisation" GetMethodModifier="Public" GetMethodName="GetDataForOrganisation" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="true" UserGetMethodName="GetDataForOrganisation" UserSourceName="GetDataForOrganisation">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        DataFlowDetailID AS DataFlowID, DFDIdentifier AS DataFlowIdentifier, DataSummaryID AS SummaryID,
                             (SELECT        DFName
                               FROM            isp_DataFlowSummaries
                               WHERE        (DataFlowID = dfd.DataSummaryID)) AS SummaryName, ArchiveDate AS Archived, ArchivedBy, DFAddedByOrgID AS AddedByOrgID,
                             (SELECT        OrganisationName
                               FROM            isp_Organisations
                               WHERE        (OrganisationID = dfd.DFAddedByOrgID)) AS AddedByOrganisation,
                             (SELECT        UserName
                               FROM            aspnet_Users
                               WHERE        (UserId = dfd.DFDAddedBy)) AS AddedBy, DFDAddedDate AS AddedDate, COALESCE
                             ((SELECT        DirectionOfFlow AS Expr1
                                 FROM            isp_DFDDirectionOfFlow
                                 WHERE        (DFDDirectionOfFlowID = dfd.DataFlowDirection)), 'Not Specified') AS DirectionOfFlow, COALESCE
                             ((SELECT        FrequencyOfTransfer
                                 FROM            isp_DFDFrequencyOfTransfer
                                 WHERE        (DFDFrequencyOfTransferID = dfd.FrequencyOfTransfer)), 'Not Specified') AS Frequency, COALESCE
                             ((SELECT        NumberOfRecords
                                 FROM            isp_DFDNumberOfRecords
                                 WHERE        (DFDNumberOfRecordsID = dfd.NumberOfRecords)), 'Not Specified') AS NumberOfRecords,
                             (SELECT        COUNT(*) AS Signatures
                               FROM            isp_DataFlowSignatures
                               WHERE        (DataFlowDetailID = dfd.DataFlowDetailID) AND (RemovedDate IS NULL) AND (WithdrawnDate IS NULL) AND (RefusedDate IS NULL)) AS Signatures,
							   (SELECT        COUNT(*) AS Signatures
                               FROM            isp_DataFlowSignatures
                               WHERE        (DataFlowDetailID = dfd.DataFlowDetailID) AND (RemovedDate IS NULL) AND (WithdrawnDate IS NULL) AND (RefusedDate IS NULL) AND (OrganisationID =  @OrganisationID)) AS MyOrgSigned,
							   dbo.GetNumberOfOrgsByDataFlowID(dfd.DataFlowDetailID) AS OrgsInvolved,
                             (SELECT        COUNT(*) AS Expr1
                               FROM            isp_DataFlowSignOffRequests
                               WHERE        (DataFlowID = dfd.DataFlowDetailID) AND (Actioned = 0)) AS SignatureRequests,
							   (SELECT MIN(SignedDate) FROM isp_DataFlowSignatures WHERE (DataFlowDetailID  = dfd.DataFlowDetailID) AND (RemovedDate IS NULL)) AS FirstSigned,
							   dbo.GetRiskRatingForFlow(dfd.DataFlowDetailID) AS RiskRating, InDraft, ArchiveDate
FROM            isp_DataFlowDetail AS dfd WHERE        ((DFAddedByOrgID = @OrganisationID) OR
                         (dfd.DataFlowDetailID IN
                             (SELECT        do.DataFlowDetailID
FROM            isp_DFD_DFDOrganisations AS do INNER JOIN
                         isp_DF_Organisations AS o ON do.DF_OrgID = o.DF_OrgID
WHERE        (o.OrganisationID = @OrganisationID)))) ORDER BY SummaryName, DFDIdentifier, DFDAddedDate</CommandText>
                    <Parameters>
