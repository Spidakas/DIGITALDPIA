            <MainSource>
              <DbSource ConnectionRef="dpiasandpitConnectionString (Web.config)" DbObjectType="Unknown" GenerateMethods="Get" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="GetData">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        o.OrganisationID, o.OrganisationName, o.OrganisationAddress, o.ISPFirstRegisteredDate, o.ICORegistrationNumber, o.OrgContactPhone, o.OrgContactEmail, 
                         o.OrgContactName, COALESCE
                             ((SELECT        OrganisationName
                                 FROM            isp_Organisations
                                 WHERE        (OrganisationID = o.SponsorOrganisationID)), 'None') AS Sponsor, COALESCE
                             ((SELECT        GroupName
                                 FROM            isp_AdminGroups
                                 WHERE        (AdminGroupID = o.AdminGroupID)), 'None') AS AdminGroup,
                             (SELECT        TOP (1) SignedDate
                               FROM            isp_TierZeroSignatures
                               WHERE        (OrganisationID = o.OrganisationID) AND (RemovedDate IS NULL)
                               ORDER BY SignedDate DESC) AS MOUSigned, COALESCE (o.AssuranceScore, - 1) AS AssuranceScore, ASub.ICORegReviewDate AS AssuranceValidTo, 
                         COALESCE
                             ((SELECT        IGComplianceType
                                 FROM            isp_IGComplianceTypes
                                 WHERE        (IGComplianceTypeID = ASub.IGComplianceTypeID)), '--') AS AssuranceFramework, ASub.IGComplianceVersion, ASub.IGComplianceScore, 
                         COALESCE (ASub.StaffScreened, 0) AS StaffScreened, COALESCE (ASub.IGTraining, 0) AS IGTraining, COALESCE
                             ((SELECT        CyberEssentialsType
                                 FROM            isp_Assurance_CyberEssentials
                                 WHERE        (CyberEssentialsID = ASub.CyberEssentialsID)), '--') AS CyberEssentialsStatus,
                             (SELECT        COUNT(DataFlowDetailID) AS DataFlows
                               FROM            isp_DataFlowDetail
                               WHERE        (ArchiveDate IS NULL) AND (DataFlowDetailID IN
                                                             (SELECT        do.DataFlowDetailID
                                                               FROM            isp_DFD_DFDOrganisations AS do INNER JOIN
                                                                                         isp_DF_Organisations AS o ON do.DF_OrgID = o.DF_OrgID
                                                               WHERE        (o.OrganisationID = @OrganisationID)))) AS DataFlows,
                             (SELECT        COUNT(og.DataFlowID) AS DataSharingSummaries
                               FROM            isp_DF_OrganisationGroups AS og INNER JOIN
                                                         isp_DF_Organisations AS o ON og.DF_OrgGroupID = o.DF_OrgGroupID INNER JOIN
                                                         isp_DataFlowSummaries AS dfs ON og.DataFlowID = dfs.DataFlowID
                               WHERE        (og.DataFlowID &gt; 0) AND (o.OrganisationID = @OrganisationID) AND (dfs.DFArchivedDate IS NULL)) AS SharingSummaries,
                             (SELECT        COUNT(DataAssetID) AS Assets
                               FROM            isp_DataAssetInventory
                               WHERE        (OrganisationID = @OrganisationID) AND (RemovedDate IS NULL)) AS DataAssets, ASub.IGNonComplianceReason, o.PrivacyNoticeURL, 
                         CASE WHEN O.DPOExempt = 1 THEN 'DPO not required' ELSE COALESCE
                             ((SELECT        TOP (1) OrganisationUserName
                                 FROM            isp_OrganisationUsers
                                 WHERE        (OrganisationID = @OrganisationID) AND (RoleID = 10) AND (Active = 1) AND (ResignDate IS NULL)
                                 ORDER BY Confirmed DESC, Validated DESC, AddedDate DESC), 'No DPO specified') END AS DPO
FROM            isp_Organisations AS o LEFT OUTER JOIN
                         isp_AssuranceSubmissions AS ASub ON o.OrganisationID = ASub.OrganisationID
WHERE        (ASub.SubmissionClosedDate IS NULL) AND (o.InactivatedDate IS NULL) AND (o.OrganisationID = @OrganisationID)</CommandText>
