              <DbSource ConnectionRef="csISPData (MySettings)" DbObjectType="Unknown" GenerateMethods="Get" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="GetData">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT       'ORG' + RIGHT('000000'+CAST(org.OrganisationID AS VARCHAR(6)),6) as ISGID, org.OrganisationName, dbo.GetFlows_MyOrg_TheirOrg(@OrganisationID, org.OrganisationID) AS Flows, 
                         CASE WHEN OrganisationTypeID = 1 THEN 'Lead' ELSE 'Sponsored' END AS OrgType, CAST(CASE WHEN
                             (SELECT        COUNT(TierZeroID) AS Signed
                               FROM            isp_TierZeroSignatures
                               WHERE        (OrganisationID = org.OrganisationID) AND (RemovedDate IS NULL)) &gt; 0 THEN 1 ELSE 0 END AS Bit) AS TierZeroSigned, org.ICORegistrationNumber, CASE WHEN org.AssuranceScore IS NULL THEN 'Not submitted' WHEN org.AssuranceScore = -10 Then 'Expired' WHEN org.AssuranceScore = 0 THEN 'Significant' WHEN org.AssuranceScore = 1 THEN 'Limited' ELSE 'None' END AS Assurance,
                             (SELECT        IGComplianceType
                               FROM            isp_IGComplianceTypes
                               WHERE        (IGComplianceTypeID = ass.IGComplianceTypeID)) AS AssuranceType, ass.ICORegReviewDate, ass.IGComplianceVersion, 
                         ass.IGComplianceScore
FROM            isp_Organisations AS org LEFT JOIN
                         isp_AssuranceSubmissions AS ass ON org.OrganisationID = ass.OrganisationID
WHERE        (org.OrganisationID &lt;&gt; @OrganisationID) AND (dbo.GetFlows_MyOrg_TheirOrg(@OrganisationID, org.OrganisationID) &gt; 0) AND (org.InactivatedDate IS NULL) AND 
                         (ass.SubmissionClosedDate IS NULL)</CommandText>
