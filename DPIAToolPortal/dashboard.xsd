                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT org.OrganisationID, 'ORG' + RIGHT('000000' + CAST(org.OrganisationID AS VARCHAR(6)), 6) AS ISGID, org.OrganisationName, 
                  dbo.GetFlows_MyOrg_TheirOrg(@OrganisationID, org.OrganisationID) AS Flows, CASE WHEN OrganisationTypeID = 1 THEN 'Lead' ELSE 'Sponsored' END AS OrgType, 
                  CAST(CASE WHEN
                      (SELECT COUNT(TierZeroID) AS Signed
                       FROM      isp_TierZeroSignatures
                       WHERE   (OrganisationID = org.OrganisationID) AND (RemovedDate IS NULL)) &gt; 0 THEN 1 ELSE 0 END AS Bit) AS TierZeroSigned, org.ICORegistrationNumber, 
                  CASE WHEN org.AssuranceScore IS NULL 
                  THEN 'Not submitted' WHEN org.AssuranceScore = - 10 THEN 'Expired' WHEN org.AssuranceScore = 0 THEN 'Significant' WHEN org.AssuranceScore = 1 THEN 'Limited' WHEN org.AssuranceScore
                   = - 11 THEN 'ISG Inactive' ELSE 'None' END AS Assurance,
                      (SELECT IGComplianceType
                       FROM      isp_IGComplianceTypes
                       WHERE   (IGComplianceTypeID = ass.IGComplianceTypeID)) AS AssuranceType, ass.ICORegReviewDate, ass.IGComplianceVersion, ass.IGComplianceScore, 
                  CASE WHEN DATEDIFF(m, ass.SubmittedDate, getDate()) &lt; 3 THEN 1 ELSE 0 END AS NewAssurance
