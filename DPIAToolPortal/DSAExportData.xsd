              <DbSource ConnectionRef="csISPData (MySettings)" DbObjectType="Unknown" GenerateMethods="Get" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="GetData">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        dfo.DFD_DFDOrganisationID,  CASE WHEN
                             (SELECT        InactivatedDate
                               FROM            isp_Organisations
                               WHERE        OrganisationID = do.OrganisationID) IS NULL THEN do.OrganisationName ELSE do.OrganisationName + ' (ISG  INACTIVE)' END AS OrganisationName, COALESCE (o.ICORegistrationNumber, do.OrganisationICONum) AS ICONumber, o.OrgContactEmail, 
                         COALESCE (o.AssuranceScore, - 1) AS Assurance, COALESCE (do.OrganisationID, 0) AS OrganisationID, 
                         isp_TierZeroSignatures.SignedDate, isp_TierZeroSignatures.SignedBy, do.Providing, do.Receiving
FROM            isp_TierZeroSignatures RIGHT OUTER JOIN
                         isp_Organisations AS o ON isp_TierZeroSignatures.OrganisationID = o.OrganisationID RIGHT OUTER JOIN
                         isp_DFD_DFDOrganisations AS dfo INNER JOIN
                         isp_DF_Organisations AS do ON dfo.DF_OrgID = do.DF_OrgID ON o.OrganisationID = do.OrganisationID
WHERE        (dfo.DataFlowDetailID = @DFDID) AND (isp_TierZeroSignatures.RemovedDate IS NULL) AND (o.InactivatedDate IS NULL)</CommandText>
                    <Parameters>
                      <Parameter AllowDbNull="false" AutogeneratedName="DFDID" ColumnName="DataFlowDetailID" DataSourceName="ispdata.dbo.isp_DFD_DFDOrganisations" DataTypeServer="int" DbType="Int32" Direction="Input" ParameterName="@DFDID" Precision="0" Scale="0" Size="4" SourceColumn="DataFlowDetailID" SourceColumnNullMapping="false" SourceVersion="Current" />
