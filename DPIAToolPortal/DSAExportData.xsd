                         CASE WHEN dfd.ThirdPartContractIGClauses = 0 THEN 'Not Applicable' WHEN dfd.ThirdPartContractIGClauses = 0 THEN 'No' ELSE 'Yes' END AS ThirdPartContractIGClauses, dfs.DFName AS SummaryName, 
                         dfs.DFPurpose AS SummaryPurpose, dfs.DFLegalGateway AS SummaryLegalGateway, dfs.DFBenefits AS SummaryBenefits, dfs.DFDataItems AS SummaryDataItems, dfs.DFReviewCycle AS SummaryReviewCycle, 
