                         + '&lt;/li&gt;&lt;/ul&gt;' AS BusinessContinuity, '&lt;ul&gt;' + dbo.DFDDisasterRecoverySelectedCSV(@DataFlowDetailID) + '&lt;/li&gt;&lt;/ul&gt;' AS DisasterRecovery, dbo.Return_NonEEAExemptions_UL(@DataFlowDetailID) AS NonEEAExemptions, 
                         dbo.svfReturnReviewDateForSummary(dfs.DataFlowID) AS ReviewDate,
                             (SELECT        MIN(S.SignedDate) AS Expr1
                               FROM            isp_DataFlowSignatures AS S INNER JOIN
                                                         isp_DataFlowDetail AS DF ON S.DataFlowDetailID = DF.DataFlowDetailID
