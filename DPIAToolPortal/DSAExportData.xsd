          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="SignaturesTableAdapter" GeneratorDataComponentClassName="SignaturesTableAdapter" Name="Signatures" UserDataComponentName="SignaturesTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="csISPData (MySettings)" DbObjectType="Unknown" GenerateMethods="Get" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="GetData">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        Q1.DFD_DFDOrganisationID, Q1.OrganisationID, Q1.OrganisationName, Q1.ICONumber, Q1.OrganisationUserName, Q1.OrganisationUserEmail, Q1.Role, 
                         Q1.Requested, Q1.OrganisationUserID, Q1.DataFlowDetailID, sig.SignedDate, sig.WithdrawnDate, sig.RefusedDate, sig.SignedBy, sig.RoleID, sig.OnBehalfOf, 
                         sig.OnBehalfOfRoleID, sig.SignOffComments,
                             (SELECT        TOP (1) Role
                               FROM            isp_Roles AS isp_Roles_1
                               WHERE        (RoleID = sig.OnBehalfOfRoleID)) AS BehalfOfRole
FROM            (SELECT        dfdo.DFD_DFDOrganisationID, dfo.OrganisationID,  CASE WHEN
                             (SELECT        InactivatedDate
                               FROM            isp_Organisations
                               WHERE        OrganisationID = dfo.OrganisationID) IS NULL THEN dfo.OrganisationName ELSE dfo.OrganisationName + ' (ISG  INACTIVE)' END AS OrganisationName, COALESCE (o.ICORegistrationNumber, dfo.OrganisationICONum) 
                                                    AS ICONumber, ou.OrganisationUserName, ou.OrganisationUserEmail, r.Role, CAST
                                                        ((SELECT        CASE WHEN COUNT(SignOffRequestID) &gt; 0 THEN 1 ELSE 0 END AS Requested
                                                            FROM            isp_DataFlowSignOffRequests
                                                            WHERE        (Actioned = 0) AND (OrganisationUserID = ou.OrganisationUserID)) AS Bit) AS Requested, ou.OrganisationUserID, dfdo.DataFlowDetailID, 
                                                    o.InactivatedDate
                          FROM            isp_Organisations AS o LEFT OUTER JOIN
                                                    isp_Roles AS r RIGHT OUTER JOIN
                                                    isp_OrganisationUsers AS ou ON r.RoleID = ou.RoleID ON o.OrganisationID = ou.OrganisationID FULL OUTER JOIN
                                                    isp_DF_Organisations AS dfo ON o.OrganisationID = dfo.OrganisationID FULL OUTER JOIN
                                                    isp_DFD_DFDOrganisations AS dfdo ON dfo.DF_OrgID = dfdo.DF_OrgID
                          WHERE        (dfdo.DataFlowDetailID = @DataFlowDetailID) AND (ou.RoleID IN (1, 2, 3, 7)) AND (ou.Active = 1)) AS Q1 LEFT OUTER JOIN
                         isp_DataFlowSignatures AS sig ON Q1.DataFlowDetailID = sig.DataFlowDetailID AND Q1.OrganisationUserID = sig.OrganisationUserID
WHERE        (sig.RemovedDate IS NULL) AND (sig.SignedDate IS NOT NULL) AND (sig.WithdrawnDate IS NULL) AND (sig.RefusedDate IS NULL)
ORDER BY Q1.OrganisationName</CommandText>
                    <Parameters>
                      <Parameter AllowDbNull="false" AutogeneratedName="DataFlowDetailID" ColumnName="" DataSourceName="" DataTypeServer="int" DbType="Int32" Direction="Input" ParameterName="@DataFlowDetailID" Precision="0" Scale="0" Size="4" SourceColumn="" SourceColumnNullMapping="false" SourceVersion="Current" />
                    </Parameters>
